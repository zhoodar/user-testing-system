<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD/EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="kg.djedai.app.models">
    <class name="TestConfig" table="config">
        <meta attribute="class-description">
            This class contains the test config detail
        </meta>
        <id name="id" type="int" column="uid">
            <generator class="identity"/>
        </id>
        <property name="durationOfQuiz" column="duration" type="int"/>
        <property name="quantityOfQuestions" column="quantity" type="int"/>
        <property name="isActive" column="is_active" type="string"/>
    </class>

</hibernate-mapping>